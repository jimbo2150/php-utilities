{
    "name": "jimbo2150/php-utilities",
    "description": "Various utilities for PHP projects or libraries.",
    "type": "library",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "Jimbo2150\\PhpUtilities\\": "src/"
        }
    },
	"autoload-dev": {
		"psr-4": {
			"Jimbo2150\\PhpUtilities\\Tests\\": "tests/"
        }
