    "authors": [
        {
            "name": "Jim Robinson",
            "email": "jimbo2150@gmail.com"
        }
    ],
	"require": {
		"php": ">=8.1",
		"ext-filter": "*"
	},
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.70",
        "phpunit/phpunit": "^10.0|^11.0|^12.0"
    }
}
